#include <stdio.h>

int main() {
    int n, indexBlock[50];

    printf("Enter number of file blocks: ");
    scanf("%d", &n);

    printf("Enter block addresses:\n");
    for(int i = 0; i < n; i++) {
        scanf("%d", &indexBlock[i]);
    }

    printf("\nIndexed File Allocation\n");
    printf("Index Block -> Data Blocks\n");

    for(int i = 0; i < n; i++) {
        printf("Index[%d] -> Block %d\n", i, indexBlock[i]);
    }

    return 0;
}
