#include <stdio.h>

int main() {
    int frames, pages;
    int page[50], frame[10];
    int fault = 0;

    printf("Enter number of frames: ");
    scanf("%d", &frames);

    printf("Enter number of pages: ");
    scanf("%d", &pages);

    printf("Enter page reference string:\n");
    for(int i = 0; i < pages; i++) {
        scanf("%d", &page[i]);
    }

    for(int i = 0; i < frames; i++) {
        frame[i] = -1;
    }

    printf("\nPage\tFrames\n");

    for(int i = 0; i < pages; i++) {
        int found = 0;

        for(int j = 0; j < frames; j++) {
            if(frame[j] == page[i]) {
                found = 1;
                break;
            }
        }

        if(!found) {
            int pos = -1, farthest = i + 1;

            for(int j = 0; j < frames; j++) {
                int k;
                for(k = i + 1; k < pages; k++) {
                    if(frame[j] == page[k])
                        break;
                }

                if(k == pages) {
                    pos = j;
                    break;
                }

                if(k > farthest) {
                    farthest = k;
                    pos = j;
                }
            }

            if(pos == -1)
                pos = 0;

            frame[pos] = page[i];
            fault++;
        }

        printf("%d\t", page[i]);
        for(int j = 0; j < frames; j++) {
            if(frame[j] == -1)
                printf("- ");
            else
                printf("%d ", frame[j]);
        }
        printf("\n");
    }

    printf("\nTotal Page Faults = %d\n", fault);
    return 0;
}
