#include <stdio.h>

int main() {
    int p = 3, r = 3;
    int alloc[3][3] = {{0,1,0},{2,0,0},{3,0,2}};
    int max[3][3] = {{7,5,3},{3,2,2},{9,0,2}};
    int avail[3] = {3,3,2};

    int need[3][3];
    for(int i=0;i<p;i++)
        for(int j=0;j<r;j++)
            need[i][j] = max[i][j] - alloc[i][j];

    printf("Need Matrix:\n");
    for(int i=0;i<p;i++) {
        for(int j=0;j<r;j++)
            printf("%d ", need[i][j]);
        printf("\n");
    }

    return 0;
}
