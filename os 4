#include <stdio.h>

int main() {
    int n, bt[10], p[10], temp;

    printf("Enter number of processes: ");
    scanf("%d", &n);

    for(int i=0;i<n;i++) {
        printf("Enter burst time P%d: ", i+1);
        scanf("%d", &bt[i]);
        p[i] = i+1;
    }

    for(int i=0;i<n-1;i++) {
        for(int j=i+1;j<n;j++) {
            if(bt[i] > bt[j]) {
                temp = bt[i];
                bt[i] = bt[j];
                bt[j] = temp;

                temp = p[i];
                p[i] = p[j];
                p[j] = temp;
            }
        }
    }

    printf("\nProcess Order: ");
    for(int i=0;i<n;i++)
        printf("P%d ", p[i]);

    return 0;
}
