#include <stdio.h>

int main() {
    int d, f;

    printf("Enter number of directories: ");
    scanf("%d", &d);

    for(int i=0;i<d;i++) {
        printf("Directory %d\n", i+1);
        printf("Enter number of files: ");
        scanf("%d", &f);

        for(int j=0;j<f;j++) {
            char name[20];
            printf("Enter file name: ");
            scanf("%s", name);
        }
    }

    return 0;
}
